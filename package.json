  "main": "./dist/bundle.js",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@colthands"
  },
  "keywords": [
    "keyword"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
